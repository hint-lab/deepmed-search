# NextAuth.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=<your-secret-key>
# Generate with: openssl rand -base64 32

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=<your-postgres-password>
POSTGRES_DB=deepmed
POSTGRES_PORT=5432
DATABASE_URL=postgresql://postgres:<your-postgres-password>@localhost:5432/deepmed

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=<your-redis-password>
# For Docker
# REDIS_HOST=deepmed-redis

# OAuth Configuration
GOOGLE_CLIENT_ID=<your-google-client-id>
GOOGLE_CLIENT_SECRET=<your-google-client-secret>
GITHUB_CLIENT_ID=<your-github-client-id>
GITHUB_CLIENT_SECRET=<your-github-client-secret>

# MinIO/S3 File Storage Configuration
MINIO_ENDPOINT=deepmed-minio
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=<your-minio-access-key>
MINIO_SECRET_KEY=<your-minio-secret-key>
MINIO_BUCKET_NAME=deepmed
MINIO_BROWSER_REDIRECT_URL=http://localhost:9001
MINIO_MAX_CONNECTIONS=10
MINIO_MAX_RETRIES=3
MINIO_RETRY_DELAY=1000
MINIO_TIMEOUT=30000

# pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=<your-pgadmin-email>
PGADMIN_DEFAULT_PASSWORD=<your-pgadmin-password>

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000

# LLM Search Library Defaults
LLM_SEARCH_DEFAULT_MAX_TOKENS=4096
LLM_SEARCH_DEFAULT_TEMPERATURE=0.5
# Optional: Model-specific overrides
# LLM_SEARCH_GPT4OMINI_MAX_TOKENS=8000
LLM_SEARCH_KNOWLEDGE_CUTOFF="October 2023"

# OpenAI Client Compatibility
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_API_KEY=<your-openai-api-key>
OPENAI_API_MODEL=gpt-4o-mini

# DeepSeek Configuration
DEEPSEEK_API_KEY=<your-deepseek-api-key>
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# Gemini Configuration
GEMINI_API_KEY=<your-gemini-api-key>
GEMINI_BASE_URL=

# Tesseract.js Configuration
# TESS_WORKER_PATH=node_modules/tesseract.js/dist/worker.min.js
# TESS_CORE_PATH=node_modules/tesseract.js-core
# TESSDATA_PREFIX=node_modules/@tesseract.js/tessdata

# Search Provider Configuration
TAVILY_API_KEY=<your-tavily-api-key>
SEARCH_PROVIDER=jina
JINA_API_KEY=<your-jina-api-key>

# Proxy Configuration
HTTP_PROXY=socks5://127.0.0.1:1080
HTTPS_PROXY=socks5://127.0.0.1:1080